{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/.idea": true,
        "**/*.js.map": true,
        "**/*.js": {
            "when": "$(basename).ts"
        },
        "**/*.gpj": true,
        "**/*.class": {
            "when": "$(basename).java"
        },
        ".history": true
    },
    "explorerExclude.backup": null
}